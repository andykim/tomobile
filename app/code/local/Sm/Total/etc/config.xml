<?xml version="1.0"?>
<config>
	<modules>
		<Sm_Total>
			<version>1.0.0</version>
			<title>MagenTech.com</title>
			<link><![CDATA[http://www.magentech.com/]]></link>
		</Sm_Total>
	</modules>
	<global>
		<helpers>
		  <total>
			<class>Sm_Total_Helper</class>
		  </total>
		</helpers>
		<models>
		  <total>
			<class>Sm_Total_Model</class>
		  </total>
