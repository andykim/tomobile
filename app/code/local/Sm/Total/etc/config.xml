	</global>
	<adminhtml>
        <translate>
            <modules>
                <Sm_Total>
                    <files>
                        <default>Sm_Total.csv</default>
                    </files>
                </Sm_Total>
            </modules>
        </translate>
	</adminhtml>	
 	<default>
